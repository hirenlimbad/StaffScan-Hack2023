{
  "builds": [
    {
      "src": "Application/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Application/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "$PYTHONPATH:/vercel/path0"
  },
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9",
      "install": {
        "include": ["requirements.txt"],
        "dest": "/vercel/path0"
      }
    }
  }
}
